+  static guint widget_type = 0;
+
+  if (!widget_type)
+    {
+      GtkTypeInfo widget_info =
+      {
+        "GXsnmp_traffic_dialog",
+        sizeof (GXsnmp_traffic_dialog),
+        sizeof (GXsnmp_traffic_dialogClass),
+        (GtkClassInitFunc) traffic_dialog_class_init,
+        (GtkObjectInitFunc) traffic_dialog_init,
+        (GtkArgSetFunc) NULL,
+        (GtkArgGetFunc) NULL
+      };
+      widget_type = gtk_type_unique (gnome_property_dialog_get_type (), 
+				     &widget_info);
+    }
+  return widget_type;
+}
